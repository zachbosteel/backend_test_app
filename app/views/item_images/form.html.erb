<%= form_for @item, url: {action: "create"}, html: {multipart: true} do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= file_field_tag "image[]", type: :file, multiple: true %>

  <%= f.submit "Create Item" %>
<% end %>
